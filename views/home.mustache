{{>scripts}}

<script>
(function(){

$('#btn-order').click(function(event){
	if (event)
		event.preventDefault()

	var order = {
		name : $('#order-name').val(),
		email : $('#order-email').val(),
		order : $('#order-desc').val()

	}
	/*console.log("Order placed !!" + JSON.stringify(order))*/

	$.ajax({
		url:'/order',
		method: 'post',
		data: order,
		success: function(resp){
			$('#btn-order-container').html('<h3>Thanks! we will get back to u.</h3>')
		}
		
	})
})

})()
</script>
    
